;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	pe.asa.br. root.pe.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	MX	mail.pe.asa.br.
@	IN	NS	ns1.pe.asa.br.

@	IN	A	127.0.0.1
@	IN	AAAA	::1

ns1			A	PRIMARY
ns2			A	SECONDARY
s1			A	PRIMARY
s2			A	SECONDARY